{
  "name": "re-magic-layout",
  "author": "Dawnlck",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.14.0",
    "react-draggable": "^4.4.3",
    "react-highlight": "^0.14.0",
    "redux": "^4.0.5",
    "umi": "^3.4.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.2",
    "babel-jest": "^27.0.6",
    "dumi": "^1.1.17",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
